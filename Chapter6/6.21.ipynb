   ],
   "source": [
    "for i in range(1, 6):\n",
    "    n = int(input(\"กรุณาป้อนตัวเลข 1-10 : \"))\n",
    "    if n >= 11:\n",
    "        print (\"คุณป้อนตัวเลขไม่ถูกต้อง !!\")\n",
    "        break\n",
    "    if n <= 10:\n",
    "        j = n * i\n",
    "    print (\"ผลคูณระหว่าง %d x %d = \" %(i, n), j)\n",
    "print (\"สิ้นสุดการทำงานของโปรแกรม\")"
   ]
